@model IEnumerable<Dog>
@{
    ViewData["Title"] = "FCI7";
}

<h1>Группа FCI 7 - </h1>
<p>
</p>
<h1>Все породы FCI Group 7</h1>
<div class="row">
    @if (Model != null && Model.Count() > 0)
    {
        @foreach (var item in Model)
        {
            @if (item.FCIGroup == "7")
            {
                <partial name="_DogCardPartial" model="@item"></partial>
            }
        }
    }

</div>
